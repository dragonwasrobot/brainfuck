{
  "name": "brainfuck",
  "version": "1.0.0",
  "description": "A brainfuck interpreter written in Elm",
  "dependencies": {
    "elm-format": "=0.8.8",
    "elm-verify-examples": "=6.0.3",
    "uglify-js": "^3.19.3"
  },
  "scripts": {
    "css": "./node_modules/.bin/tailwindcss -i docs/style.css -o docs/main.css",
    "dev": "./node_modules/.bin/elm-watch hot",
    "format": "./node_modules/.bin/elm-format --validate src",
    "test": "./node_modules/.bin/elm-test tests",
    "test-watch": "./node_modules/.bin/elm-test --watch",
    "tailwind-watch": "npx @tailwindcss/cli -i docs/style.css -o docs/main.css --watch"
  },
  "devDependencies": {
    "elm-test": "^0.19.1-revision16",
    "elm-watch": "^1.2.2",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@tailwindcss/cli": "^4.1.4",
    "tailwindcss": "^4.1.4"
  }
}
