{
  "name": "brainfuck",
  "version": "1.0.0",
  "description": "A brainfuck interpreter written in Elm",
  "dependencies": {
    "elm-format": "=0.8.8",
    "elm-verify-examples": "=6.0.3",
    "uglify-js": "^3.19.3"
  },
  "scripts": {
    "css": "npx @tailwindcss/cli -i docs/style.css -o docs/main.css",
    "css-watch": "npx @tailwindcss/cli -i docs/style.css -o docs/main.css --watch",
    "compile": "npx elm-watch make",
    "compile-watch": "npx elm-watch hot",
    "local-server": "cd docs && python3 -m http.server",
    "format": "npx elm-format --validate src",
    "test": "npx elm-test tests",
    "test-watch": "npx elm-test --watch"
  },
  "devDependencies": {
    "elm-test": "^0.19.1-revision16",
    "elm-watch": "^1.2.3",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@tailwindcss/cli": "^4.1.16",
    "tailwindcss": "^4.1.15"
  }
}
