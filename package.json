{
  "name": "brainfuck",
  "version": "1.0.0",
  "description": "A brainfuck interpreter written in Elm",
  "dependencies": {
    "elm-format": "=0.8.8",
    "elm-verify-examples": "=6.0.3",
    "uglify-js": "^3.19.3"
  },
  "scripts": {
    "css": "npx @tailwindcss/cli -i docs/style.css -o docs/main.css",
    "compile": "elm make --debug --output=docs/elm.min.js src/Main.elm",
    "serve": "cd docs && python3 -m http.server",
    "format": "npx elm-format --validate src",
    "test": "npx elm-test tests"
  },
  "devDependencies": {
    "elm-test": "^0.19.1-revision17",
    "@fortawesome/fontawesome-free": "^7.2.0",
    "@tailwindcss/cli": "^4.2.1",
    "tailwindcss": "^4.2.0"
  }
}
